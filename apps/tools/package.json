{
  "name": "@youbiquti/tools",
  "version": "0.0.1",
  "private": true,
  "description": "GNO Tool Azure Functions",
  "main": "dist/index.js",
  "scripts": {
    "build": "node esbuild.config.mjs",
    "build:tsc": "tsc",
    "watch": "tsc -w",
    "start": "func start",
    "predeploy": "npm run build",
    "deploy": "func azure functionapp publish gno-tools-func",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@azure/functions": "^4.5.0",
    "@youbiquti/core": "workspace:*",
    "@youbiquti/gno-tools": "workspace:*",
    "@youbiquti/service-graph": "workspace:*"
  },
  "devDependencies": {
    "azure-functions-core-tools": "^4.0.5571",
    "esbuild": "^0.20.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.0"
  }
}
